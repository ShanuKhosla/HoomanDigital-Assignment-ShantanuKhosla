---
import type {CollectionEntry} from "astro:content";
type Props = {
    portfolios: CollectionEntry<'portfolios'>[];
};
import PortfolioCard from "./PortfolioCard.astro"
const {portfolios} = Astro.props;
---

<div class="grid md:grid-cols-3 grid-cols-1 gap-10 my-10">
    {portfolios.map((portfolio) => <PortfolioCard portfolio={portfolio} />)}
</div>