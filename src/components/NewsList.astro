---
import type {CollectionEntry} from "astro:content";
import NewsCard from "./NewsCard.astro";
type Props = {
    latestnews: CollectionEntry<'latestnews'>[];
};

const {latestnews} = Astro.props;

---

<div class="grid md:grid-cols-3 grid-cols-1 gap-10 my-10 px-7">
    {latestnews.map((latestnew) => <NewsCard latestnew={latestnew} />)}
</div>